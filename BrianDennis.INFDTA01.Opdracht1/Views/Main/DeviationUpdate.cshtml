@model List<DeviationUpdateModel>

@{
    ViewBag.Title = "Deviation Update";
}

<h2>Deviation- @ViewContext.RouteData.Values["view"]</h2>

@foreach (var deviation in Model)
{
    <ul>
        <li>MoviePair: I: @deviation.UpdatePair.Item1, J: @deviation.UpdatePair.Item2</li>
        <li>Given Rating: I: @deviation.RatingI, J: @deviation.RatingJ</li>

        <li>Old CurrentDeviation: @deviation.OldDeviation</li>
        <li>New CurrenDeviation: @deviation.NewDeviation</li>
    </ul>
}
