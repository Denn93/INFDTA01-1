@model List<DeviationResultItem>

@{
    ViewBag.Title = "Deviation";
}

<h2>Deviation- @ViewContext.RouteData.Values["view"]</h2>

@foreach (DeviationResultItem item in Model)
{
    <ul>
        <li>Movie 1: @item.I</li>
        <li>Movie 2: @item.J</li>
        <li>Deviation: @item.Deviation</li>
        <li>Number of users  rated both items @item.UserCount</li>
    </ul>
}