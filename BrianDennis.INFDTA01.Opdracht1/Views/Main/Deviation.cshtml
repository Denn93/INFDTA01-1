@model Tuple<int, Dictionary<int, double>>

@{
    ViewBag.Title = "Predicted rating slope one";
}

<h2>Deviation- @ViewContext.RouteData.Values["view"]</h2>

@foreach (KeyValuePair<int, double> item in Model.Item2)
{
    <ul>
        <li>Movie: @item.Key</li>
        <li>Predicted Rating: @item.Value</li>
    </ul>
}